                "./dist/*.min.js",
                "./dist/*.min.map"
            ]
        },
        "json3": {
            "main": "./**/*.min.js"
        },
        "lodash": {
            "main": "./**/*.min.js"
        },
        "moment": {
            "main": "./**/*-locales.min.js"
        },
        "ng-table-extend": {
            "main": [
                "./**/*.min.js",
                "./**/*.min.css",
                "./**/*.map"
            ]
        },
        "socket.io-client": {
            "main": [
                "./socket.io.js"
            ]
        }
    },
    "resolutions": {
        "angular": "~1.3.0"
    },
    "name": "angwebserver",
