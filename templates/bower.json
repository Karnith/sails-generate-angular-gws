{
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "angular-rx-data-table": "*",
        "ng-table": "*",
        "fontawesome": "*",
        "jquery": "*"
    },
    "devDependencies": {
        "angular": "*",
        "angular-bootstrap": "*",
        "angular-mocks": "*",
        "angular-moment": "*",
        "angular-sails": "*",
        "angular-translate": "*",
        "angular-translate-loader-static-files": "*",
